<?xml version="1.0" encoding="UTF-8"?>

<web-app>
   <servlet>
   	   <servlet-name>displayCourses</servlet-name>
       <servlet-class>com.tmf.sfe.servlets.DisplayCourses</servlet-class>
   </servlet>
   <servlet-mapping>
   	   <servlet-name>displayCourses</servlet-name>
   	   <url-pattern>/displayCourses</url-pattern>
   </servlet-mapping>
   
   <servlet>
   	   <servlet-name>addCourse</servlet-name>
       <servlet-class>com.tmf.sfe.servlets.AddCourse</servlet-class>
   </servlet>
   <servlet-mapping>
   	   <servlet-name>addCourse</servlet-name>
   	   <url-pattern>/saveCourse</url-pattern>
   </servlet-mapping>
</web-app>